namespace Sakura {
    namespace Core {
        namespace Types {

            template <typename T>
            class CompositeLexicalType;

            typedef CompositeLexicalType<const char*> String;

        }
    }
}
#endif

#ifndef LIBRARY
#include <Core/Types/String.hpp>
#endif

#endif

